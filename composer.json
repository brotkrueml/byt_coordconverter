{
    "name": "brotkrueml/byt_coordconverter",
    "type": "typo3-cms-extension",
    "description": "TYPO3 view helper for converting geospatial coordinates from one format into another",
    "homepage": "https://github.com/brotkrueml/byt_coordconverter",
    "license": [
        "GPL-2.0+"
    ],
    "keywords": [
        "TYPO3",
        "extension",
        "coordinate converter"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/byt_coordconverter/issues"
    },
    "require": {
        "typo3/cms-core": "^8.7 || ^9.5",
        "typo3/cms-fluid": "^8.7 || ^9.5"
    },
    "require-dev": {
        "nimut/testing-framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\Tests\\": "Tests"
        }
    },
    "replace": {
        "byt_coordconverter": "self.version",
        "typo3-ter/byt_coordconverter": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/web",
            "extension-key": "byt_coordconverter"
        }
    }
}
