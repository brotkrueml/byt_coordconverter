{
    "name": "brotkrueml/coordconverter",
    "type": "typo3-cms-extension",
    "description": "TYPO3 view helper for converting geospatial coordinates from one format into another",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "coordinate converter",
        "coordinates"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/byt_coordconverter/issues"
    },
    "require": {
        "typo3/cms-core": "^8.7.25 || ^9.5 || 10.0.*",
        "typo3fluid/fluid": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.5"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/byt_coordconverter": "self.version"
    },
    "config": {
        "sort-packages": true,
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build/web",
            "extension-key": "byt_coordconverter"
        }
    }
}
