{
    "name": "brotkrueml/coordconverter",
    "description": "TYPO3 view helper for converting geospatial coordinates from one format into another",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "coordinate converter",
        "coordinates",
        "utm",
        "wgs84"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "homepage": "https://extensions.typo3.org/extension/byt_coordconverter",
    "support": {
        "docs": "https://docs.typo3.org/p/brotkrueml/coordconverter/main/en-us/",
        "issues": "https://github.com/brotkrueml/byt_coordconverter/issues",
        "source": "https://github.com/brotkrueml/byt_coordconverter"
    },
    "require": {
        "php": ">=7.4",
        "typo3/cms-core": "^9.5 || ^10.4 || ^11.5",
        "typo3fluid/fluid": "^2.6"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~1.2.0",
        "ergebnis/composer-normalize": "~2.24.0",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "ergebnis/composer-normalize": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "byt_coordconverter",
            "web-dir": ".Build/web"
        }
    }
}
