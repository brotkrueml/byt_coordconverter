{
    "name": "brotkrueml/coordconverter",
    "description": "TYPO3 view helper for converting geospatial coordinates from one format into another",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "coordinate converter",
        "coordinates",
        "utm",
        "wgs84"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/byt_coordconverter/issues",
        "docs": "https://docs.typo3.org/p/brotkrueml/coordconverter/main/en-us/"
    },
    "require": {
        "php": ">=7.4",
        "typo3/cms-core": "^10.4.11 || ^11.5 || ^12.0",
        "typo3fluid/fluid": "^2.6"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~2.0.2",
        "ergebnis/composer-normalize": "~2.28.3",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "ergebnis/composer-normalize": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "byt_coordconverter",
            "web-dir": ".Build/web"
        }
    }
}
