{
    "name": "brotkrueml/coordconverter",
    "description": "TYPO3 view helper for converting geospatial coordinates from one format into another",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "coordinate converter",
        "coordinates",
        "utm",
        "wgs84"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/byt_coordconverter/issues",
        "docs": "https://docs.typo3.org/p/brotkrueml/coordconverter/main/en-us/"
    },
    "require": {
        "php": ">=7.4",
        "typo3/cms-core": "^11.5 || ^12.4 || ^13.0",
        "typo3fluid/fluid": "^2.7"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~3.0.0",
        "ergebnis/composer-normalize": "~2.42.0",
        "php-coveralls/php-coveralls": "^2.7",
        "phpstan/phpstan": "1.10.57",
        "phpunit/phpunit": "^9.6",
        "rector/rector": "1.0.0"
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.2.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "byt_coordconverter",
            "web-dir": ".Build/web"
        }
    }
}
