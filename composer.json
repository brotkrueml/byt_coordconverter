{
    "name": "brotkrueml/coordconverter",
    "description": "TYPO3 view helper for converting geospatial coordinates from one format into another",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "coordinate converter",
        "coordinates",
        "utm",
        "wgs84"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/byt_coordconverter/issues",
        "docs": "https://docs.typo3.org/p/brotkrueml/coordconverter/main/en-us/"
    },
    "require": {
        "php": ">=8.1",
        "typo3/cms-core": "^12.4 || ^13.4",
        "typo3/cms-fluid": "^12.4 || ^13.4"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~6.3.0",
        "ergebnis/composer-normalize": "~2.48.2",
        "phpstan/phpstan": "2.1.32",
        "phpunit/phpunit": "^10.5 || ^11.5 || ^12.4",
        "rector/rector": "2.2.10"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\BytCoordconverter\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.4.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "byt_coordconverter",
            "web-dir": ".Build/web"
        }
    }
}
