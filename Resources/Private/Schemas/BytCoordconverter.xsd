<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://typo3.org/ns/Brotkrueml/BytCoordconverter/ViewHelpers"><xsd:element name="coordinateConverter"><xsd:annotation><xsd:documentation><![CDATA[ViewHelper to convert coordinates from one format into another]]></xsd:documentation></xsd:annotation><xsd:complexType mixed="true"><xsd:sequence><xsd:any minOccurs="0" maxOccurs="unbounded"/></xsd:sequence><xsd:attribute type="xsd:string" name="latitude" use="required"><xsd:annotation><xsd:documentation><![CDATA[The latitude to be converted (in decimal notation)]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="longitude" use="required"><xsd:annotation><xsd:documentation><![CDATA[The longitude to be converted (in decimal notation)]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="outputFormat"><xsd:annotation><xsd:documentation><![CDATA[The format of the coordinates for the output (possible values: degree, degreeMinutes, degreeMinutesSeconds, UTM]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="cardinalPoints"><xsd:annotation><xsd:documentation><![CDATA[The naming of the cardinal points (North, South, East, West) separated by | character]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="cardinalPointsPosition"><xsd:annotation><xsd:documentation><![CDATA[The position of the cardinal points (possible values: before, after)]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="numberOfDecimals"><xsd:annotation><xsd:documentation><![CDATA[The number of decimals to be shown]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="removeTrailingZeros"><xsd:annotation><xsd:documentation><![CDATA[Should trailing zeros be removed]]></xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute type="xsd:string" name="delimiter"><xsd:annotation><xsd:documentation><![CDATA[The delimiter of the coordinates]]></xsd:documentation></xsd:annotation></xsd:attribute></xsd:complexType></xsd:element></xsd:schema>